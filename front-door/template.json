{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "frontdoors_easyfiel_production_name": {
            "defaultValue": "easyfiel-production",
            "type": "String"
        },
        "frontdoorwebapplicationfirewallpolicies_firewallPolicy_externalid": {
            "defaultValue": "/subscriptions/007d325e-01d0-43cf-a35b-03dd3a192e49/resourceGroups/easy_fiel_production_rg/providers/Microsoft.Network/frontdoorwebapplicationfirewallpolicies/firewallPolicy",
            "type": "String"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.Network/frontdoors",
            "apiVersion": "2020-05-01",
            "name": "[parameters('frontdoors_easyfiel_production_name')]",
            "location": "Global",
            "properties": {
                "resourceState": "Enabled",
                "backendPools": [
                    {
                        "id": "[concat(resourceId('Microsoft.Network/frontdoors', parameters('frontdoors_easyfiel_production_name')), '/BackendPools/pool1')]",
                        "name": "pool1",
                        "properties": {
                            "backends": [
                                {
                                    "address": "[concat(parameters('frontdoors_easyfiel_production_name'), '-secondary.azurewebsites.net')]",
                                    "httpPort": 80,
                                    "httpsPort": 443,
                                    "priority": 1,
                                    "weight": 50,
                                    "backendHostHeader": "[concat(parameters('frontdoors_easyfiel_production_name'), '-secondary.azurewebsites.net')]",
                                    "enabledState": "Enabled"
                                },
                                {
                                    "address": "[concat(parameters('frontdoors_easyfiel_production_name'), '.azurewebsites.net')]",
                                    "httpPort": 80,
                                    "httpsPort": 443,
                                    "priority": 1,
                                    "weight": 50,
                                    "backendHostHeader": "[concat(parameters('frontdoors_easyfiel_production_name'), '.azurewebsites.net')]",
                                    "enabledState": "Enabled"
                                }
                            ],
                            "healthProbeSettings": {
                                "id": "[concat(resourceId('Microsoft.Network/frontdoors', parameters('frontdoors_easyfiel_production_name')), '/HealthProbeSettings/healthProbeSettings-1607738048500')]"
                            },
                            "loadBalancingSettings": {
                                "id": "[concat(resourceId('Microsoft.Network/frontdoors', parameters('frontdoors_easyfiel_production_name')), '/LoadBalancingSettings/loadBalancingSettings-1607738048500')]"
                            },
                            "resourceState": "Enabled"
                        }
                    }
                ],
                "healthProbeSettings": [
                    {
                        "id": "[concat(resourceId('Microsoft.Network/frontdoors', parameters('frontdoors_easyfiel_production_name')), '/HealthProbeSettings/healthProbeSettings-1607738048500')]",
                        "name": "healthProbeSettings-1607738048500",
                        "properties": {
                            "intervalInSeconds": 30,
                            "path": "/",
                            "protocol": "Https",
                            "resourceState": "Enabled",
                            "enabledState": "Enabled",
                            "healthProbeMethod": "Head"
                        }
                    }
                ],
                "frontendEndpoints": [
                    {
                        "id": "[concat(resourceId('Microsoft.Network/frontdoors', parameters('frontdoors_easyfiel_production_name')), concat('/FrontendEndpoints/', parameters('frontdoors_easyfiel_production_name'), '-azurefd-net'))]",
                        "name": "[concat(parameters('frontdoors_easyfiel_production_name'), '-azurefd-net')]",
                        "properties": {
                            "hostName": "[concat(parameters('frontdoors_easyfiel_production_name'), '.azurefd.net')]",
                            "sessionAffinityEnabledState": "Enabled",
                            "sessionAffinityTtlSeconds": 0,
                            "webApplicationFirewallPolicyLink": {
                                "id": "[parameters('frontdoorwebapplicationfirewallpolicies_firewallPolicy_externalid')]"
                            },
                            "resourceState": "Enabled"
                        }
                    }
                ],
                "loadBalancingSettings": [
                    {
                        "id": "[concat(resourceId('Microsoft.Network/frontdoors', parameters('frontdoors_easyfiel_production_name')), '/LoadBalancingSettings/loadBalancingSettings-1607738048500')]",
                        "name": "loadBalancingSettings-1607738048500",
                        "properties": {
                            "additionalLatencyMilliseconds": 0,
                            "sampleSize": 4,
                            "successfulSamplesRequired": 2,
                            "resourceState": "Enabled"
                        }
                    }
                ],
                "routingRules": [
                    {
                        "id": "[concat(resourceId('Microsoft.Network/frontdoors', parameters('frontdoors_easyfiel_production_name')), '/RoutingRules/rule1')]",
                        "name": "rule1",
                        "properties": {
                            "frontendEndpoints": [
                                {
                                    "id": "[concat(resourceId('Microsoft.Network/frontdoors', parameters('frontdoors_easyfiel_production_name')), concat('/FrontendEndpoints/', parameters('frontdoors_easyfiel_production_name'), '-azurefd-net'))]"
                                }
                            ],
                            "acceptedProtocols": [
                                "Http",
                                "Https"
                            ],
                            "patternsToMatch": [
                                "/*"
                            ],
                            "enabledState": "Enabled",
                            "resourceState": "Enabled",
                            "routeConfiguration": {
                                "@odata.type": "#Microsoft.Azure.FrontDoor.Models.FrontdoorForwardingConfiguration",
                                "forwardingProtocol": "HttpsOnly",
                                "backendPool": {
                                    "id": "[concat(resourceId('Microsoft.Network/frontdoors', parameters('frontdoors_easyfiel_production_name')), '/BackendPools/pool1')]"
                                }
                            }
                        }
                    }
                ],
                "backendPoolsSettings": {
                    "enforceCertificateNameCheck": "Enabled",
                    "sendRecvTimeoutSeconds": 30
                },
                "enabledState": "Enabled",
                "friendlyName": "[parameters('frontdoors_easyfiel_production_name')]"
            }
        }
    ]
}